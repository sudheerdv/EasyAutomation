
public class ArrayDemo 
{
	//Array is a Non primitivr data type
	public static void main(String[] args) 
	{
		int[] a=new int[5];
		
		a[0]=3;
		a[1]=4;
		a[2]=5;
		a[3]=6;
		a[4]=8;
		
		int f=a[0];
		int l=a.length-1;
		int m=l/2;
		System.out.println(f);
		
		System.out.println(a[m]);
		
		System.out.println(a[l]);
		
	}
}
